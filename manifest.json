{
  "name": "Karl",
  "description": "A recreation of Franz App for Chrome OS",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "externally_connectable": {
      "matches": [ "https://*.slack.com/*" ]
   },
  "permissions": [ "storage", "webview" ],
  "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}